    "individualBeneficiariesType": {
      "description": "Individual Beneficiaries Type",
      "additionalProperties": false,
      "type": "array",
      "items": {
        "additionalProperties": false,
        "properties": {
          "individual": {
            "$ref": "#/definitions/individualType"
          },
          "isVulnerable": {
            "type": "boolean"
          },
          "incomeDistribution": {
            "$ref": "#/definitions/incomeType"
          }
        },
        "type": "object",
        "required": [
          "individual",
          "isVulnerable",
          "incomeDistribution"
        ]
      },
      "minItems": 1
    },
    "individualType": {
      "description": "Individual Type",
      "additionalProperties": false,
      "properties": {
        "correspondenceAddress": {
          "$ref": "#/definitions/correspondenceAddressType"
        },
        "dateOfBirth": {
          "type": "string",
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$"
        },
        "familyName": {
          "maxLength": 47,
          "type": "string"
        },
        "givenName": {
          "maxLength": 25,
          "type": "string"
        },
        "isUkNationalOrNonUkWithANino": {
          "type": "boolean"
        },
        "nino": {
          "pattern": "^(?!BG)(?!GB)(?!NK)(?!KN)(?!TN)(?!NT)(?!ZZ)(?:[A-CEGHJ-PR-TW-Z][A-CEGHJ-NPR-TW-Z])(?:\\d){6}([A-D]|\\s)?$",
          "type": "string"
        },
        "otherName": {
          "maxLength": 25,
          "type": "string"
        },
        "passportOrIdCard": {
          "additionalProperties": false,
          "properties": {
            "countryOfIssue": {
              "$ref": "#/definitions/countryCodeType"
            },
            "expiryDate": {
              "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
              "type": "string"
            },
            "referenceNumber": {
              "maxLength": 30,
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "required": [
        "givenName",
        "familyName",
        "dateOfBirth"
      ],
      "type": "object"
    },
