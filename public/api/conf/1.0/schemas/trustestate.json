    "deceasedType": {
      "description": "Deceased Individual Type",
      "additionalProperties": false,
      "properties": {
        "dateOfDeath": {
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
          "type": "string"
        },
        "individual": {
          "$ref": "#/definitions/individualType"
        }
      },
      "type": "object",
      "required": [
        "dateOfDeath",
        "individual"
      ]
    },
    "declarationType": {
      "description": "Declaration Type",
      "additionalProperties": false,
      "properties": {
        "correspondenceAddress": {
          "$ref": "#/definitions/correspondenceAddressType"
        },
        "confirmation": {
          "type": "boolean"
        },
        "date": {
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
          "type": "string"
        },
        "familyName": {
          "maxLength": 47,
          "type": "string"
        },
        "givenName": {
          "maxLength": 25,
          "type": "string"
        },
        "otherName": {
          "maxLength": 25,
          "type": "string"
        }
