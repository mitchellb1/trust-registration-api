    "employmentTrustType": {
      "description": "Employee Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "businessAssets": {
              "$ref": "#/definitions/businessAssetsType"
            },
            "monetaryAssets": {
              "$ref": "#/definitions/monetaryAssetsType"
            },
            "propertyAssets": {
              "$ref": "#/definitions/propertyAssetsType"
            },
            "shareAssets": {
              "$ref": "#/definitions/shareAssetsType"
            }
          },
          "type": "object",
          "anyOf": [
            {"required": ["monetaryAssets"]},
            {"required": ["propertyAssets"]},
            {"required": ["shareAssets"]},
            {"required": ["businessAssets"]}
          ]
        },
        "beneficiaries": {
          "additionalProperties": false,
          "properties": {
            "directorBeneficiaries": {
              "$ref": "#/definitions/directorBeneficiariesType"
            },
            "employeeBeneficiaries": {
              "$ref": "#/definitions/employeeBeneficiariesType"
            },
            "individualBeneficiaries": {
              "$ref": "#/definitions/individualBeneficiariesType"
            },
            "companyBeneficiaries": {
              "$ref": "#/definitions/companyBeneficiariesType"
            },
            "otherBeneficiaries": {
              "$ref": "#/definitions/otherBeneficiariesType"
            },
            "trustBeneficiaries": {
              "$ref": "#/definitions/trustBeneficiariesType"
            },
            "unidentifiedBeneficiaries": {
              "$ref": "#/definitions/unidentifiedBeneficiariesType"
            },
            "largeNumbersCompanyBeneficiaries": {
              "$ref": "#/definitions/largeNumbersCompanyBeneficiariesType"
            }
          },
          "type": "object",
          "anyOf": [
            {"required": ["individualBeneficiaries"]},
            {"required": ["directorBeneficiaries"]},
            {"required": ["otherBeneficiaries"]},
            {"required": ["trustBeneficiaries"]},
            {"required": ["companyBeneficiaries"]},
            {"required": ["unidentifiedBeneficiaries"]},
            {"required": ["employeeBeneficiaries"]},
            {"required": ["largeNumbersCompanyBeneficiaries"]}
          ]
        },
        "employerFinancedRetirementBenefitSchemeStartDate": {
          "type": "string",
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$"
        }
      },
      "required": [
        "assets",
        "beneficiaries"
      ],
      "type": "object"
    },
    "estateType": {
      "description": "Estate Type",
      "additionalProperties": false,
      "properties": {
        "estateName": {
          "type": "string"
        },
        "adminPeriodFinishedDate": {
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
          "type": "string"
        },
        "deceased": {
          "$ref": "#/definitions/deceasedType"
        },
        "reasonEstateSetup": {
          "description" : " A reason the Estate is being set up\na) Is the total Income Tax and Capital Gains Tax due for the administration period more than £10,000? ????\nb) If the date of death was before 5 April 2016, did more than £250,000 come from the sale of the estate's assets by administrators or executors, in any one tax year? ????\nc) If the date of death is on or after 6 April 2016, did more than £500,000 come from the sale of the estate's assets by administrators or executors in any one tax year? ???\nd) Was the estate worth more than £2.5 million at the date of death? ????",
          "enum": [
            "incomeTaxDueMoreThan10000",
            "saleOfEstateAssetsMoreThan250000",
            "saleOfEstateAssetsMoreThan500000",
            "worthMoreThanTwoAndHalfMillionAtTimeOfDeath"
          ]
        },
        "isCreatedByWill": {
          "type": "boolean"
        },
        "personalRepresentative": {
          "$ref": "#/definitions/personalRepresentativeType"
        }
      },
      "required": ["reasonEstateSetup"],
      "oneOf": [
        "personalRepresentative",
        "deceased"
      ],
      "type": "object"
    },
    "flatManagementSinkingFundTrustType": {
      "description": "Flat Management or Sinking Fund Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "monetaryAssets": {
