<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2017 sp1 (http://www.altova.com) by D Wright (opencast) -->
<xsi:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema">
	<xsi:element name="trustEstate">
		<xsi:annotation>
			<xsi:documentation>Root of the Trusts and Estates schema</xsi:documentation>
		</xsi:annotation>
		<xsi:complexType>
			<xsi:choice>
				<xsi:element name="trust" type="trustType" minOccurs="0" maxOccurs="1"/>
				<xsi:element name="estate" type="estateType" minOccurs="0" maxOccurs="1"/>
			</xsi:choice>
		</xsi:complexType>
	</xsi:element>
	<xsi:complexType name="trustType">
		<xsi:annotation>
			<xsi:documentation>A Trust</xsi:documentation>
		</xsi:annotation>
		<xsi:all>
			<xsi:element name="trustName" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="56"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="telephoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="currentYear" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="4"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="commencementDate" type="xsi:date" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="legality" type="legalityType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isTrustUkResident" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isTCGA925A" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isS218IHTA84" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isNonResTypeIHTA84S218" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="nonResidentType" type="residentTypeType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="leadTrustee" type="leadTrusteeType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="trustees" type="trusteesType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="protectors" type="protectorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="naturalPeople" type="naturalPeopleType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="declaration" type="declarationType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="trustTypeType">
				<xsi:complexType>
					<xsi:choice>
						<xsi:element name="willIntestacyTrust" type="willIntestacyTrustType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="interVivoTrust" type="interVivoTrustType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="heritageMaintenanceFundTrust" type="heritageMaintenanceFundTrustType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="employmentTrust" type="employmentTrustType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="flatManagementSinkingFundTrust" type="flatManagementSinkingFundTrustType" minOccurs="0" maxOccurs="1"/>
					</xsi:choice>
				</xsi:complexType>
			</xsi:element>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="estateType">
		<xsi:annotation>
			<xsi:documentation>An Estate</xsi:documentation>
		</xsi:annotation>
		<xsi:all>
			<xsi:element name="deceased" type="deceasedType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="personalRepresentative" type="personalRepresentativeType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isCreatedByWill" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="estateCriteriaMet" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="adminPeriodFinishedDate" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="incomeTaxDueMoreThan10000" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="saleOfEstateAssetsMoreThan250000" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="saleOfEstateAssetsMoreThan500000" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="worthMoreThanTwoAndHalfMillionAtTimeOfDeath" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="willIntestacyTrustType">
		<xsi:all>
			<xsi:element name="assets" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="propertyAssets" type="propertyAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="businessAssets" type="businessAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="shareAssets" type="shareAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherAssets" type="otherAssetsType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="settlors" type="settlorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="deceased" type="deceasedType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="beneficiaries" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="individualBeneficiaries" type="individualBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="charityBeneficiaries" type="charityBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherBeneficiaries" type="otherBeneficiariesType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="interVivoTrustType">
		<xsi:all>
			<xsi:element name="assets" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="propertyAssets" type="propertyAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="shareAssets" type="shareAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="businessAssets" type="businessAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="partnershipAssets" type="partnershipAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherAssets" type="otherAssetsType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="settlors" type="settlorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="beneficiaries" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="individualBeneficiaries" type="individualBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="charityBeneficiaries" type="charityBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherBeneficiaries" type="otherBeneficiariesType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="isHoldOverClaimed" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="heritageMaintenanceFundTrustType">
		<xsi:all>
			<xsi:element name="assets" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="propertyAssets" type="propertyAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="shareAssets" type="shareAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherAssets" type="otherAssetsType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="settlors" type="settlorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="beneficiaries" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="buildingLandBeneficiary" type="buildingLandBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherBeneficiaries" type="otherBeneficiariesType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="isMultiPurposeIncome" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="flatManagementSinkingFundTrustType">
		<xsi:all>
			<xsi:element name="assets" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:all>
						<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
					</xsi:all>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="settlors" type="settlorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="beneficiaries" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="buildingLandBeneficiary" type="buildingLandBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherBeneficiaries" type="otherBeneficiariesType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="employmentTrustType">
		<xsi:all>
			<xsi:element name="assets" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="propertyAssets" type="propertyAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="shareAssets" type="shareAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="businessAssets" type="businessAssetsType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherAssets" type="otherAssetsType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="settlors" type="settlorsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="beneficiaries" minOccurs="1" maxOccurs="1">
				<xsi:complexType>
					<xsi:sequence>
						<xsi:element name="individualBeneficiaries" type="individualBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="employeeBeneficiaries" type="employeeBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="directorBeneficiaries" type="directorBeneficiariesType" minOccurs="0" maxOccurs="1"/>
						<xsi:element name="otherBeneficiaries" type="otherBeneficiariesType" minOccurs="0" maxOccurs="1"/>
					</xsi:sequence>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="isEmployerFinancedRetirementBenefitScheme" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="employerFinancedRetirementBenefitSchemeStartDate" type="xsi:date" minOccurs="0" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="legalityType">
		<xsi:all>
			<xsi:element name="governingCountryCode" type="countryCodeType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="administrationCountryCode" type="countryCodeType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="previousOffshoreCountryCode" type="countryCodeType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="isEstablishedUnderScottishLaw" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="individualType">
		<xsi:all minOccurs="1">
			<xsi:element name="title" type="xsi:string" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="givenName" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="25"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="otherName" minOccurs="0" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="25"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="familyName" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="25"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="dateOfBirth" type="xsi:date" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="nino" minOccurs="0" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="9"/>
						<!--<xsi:pattern value=""/> -->
						<!-- Find pattern match for nino from hmrc -->
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="passportOrIdCard" minOccurs="0" maxOccurs="1">
				<xsi:complexType>
					<xsi:all>
						<xsi:element name="referenceNumber" minOccurs="1" maxOccurs="1">
							<xsi:simpleType>
								<xsi:restriction base="xsi:string">
									<xsi:length value="30"/>
								</xsi:restriction>
							</xsi:simpleType>
						</xsi:element>
						<xsi:element name="expiryDate" type="xsi:date"/>
						<xsi:element name="countryOfIssue" type="countryCodeType"/>
					</xsi:all>
				</xsi:complexType>
			</xsi:element>
			<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="telephoneNumber" type="telephoneNumberType" minOccurs="0" maxOccurs="1"/>
			<!-- can we make this 'nicer'? is it required for all individuals or just lead trustee? -> not required for trustees -->
			<xsi:element name="isUkNationalOrNonUkWithANino" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="deceasedType">
		<xsi:all>
			<xsi:element name="individual" type="individualType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="dateOfDeath" type="xsi:date" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="0" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="personalRepresentativeType">
		<xsi:all>
			<xsi:element name="individual" type="individualType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="telephoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="isExecutor" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="leadTrusteeType">
		<xsi:choice>
			<xsi:element name="individual" type="individualType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="company" type="companyType" minOccurs="0" maxOccurs="1"/>
		</xsi:choice>
	</xsi:complexType>
	<xsi:complexType name="trusteesType">
		<xsi:sequence>
			<xsi:element name="individuals" type="individualType" minOccurs="0" maxOccurs="unbounded"/>
			<xsi:element name="companies" type="companyType" minOccurs="0" maxOccurs="unbounded"/>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="protectorsType">
		<xsi:sequence>
			<xsi:element name="individuals" type="individualType" minOccurs="0" maxOccurs="2"/>
			<xsi:element name="companies" type="companyType" minOccurs="0" maxOccurs="2"/>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="naturalPeopleType">
		<xsi:sequence>
			<xsi:element name="individuals" type="individualType" minOccurs="0" maxOccurs="unbounded"/>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="declarationType">
		<xsi:all>
			<xsi:element name="title" type="xsi:string" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="givenName" type="xsi:string" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="otherName" type="xsi:string" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="familyName" type="xsi:string" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="capacity" type="roleType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="dateOfDeclaration" type="xsi:date" minOccurs="1" maxOccurs="1"/>
			<!-- Confirmation that the information given is true -->
			<xsi:element name="confirmation" type="xsi:boolean" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="settlorsType">
		<xsi:sequence>
			<xsi:element name="individuals" type="individualType" minOccurs="0" maxOccurs="unbounded"/>
			<xsi:element name="companies" type="companyType" minOccurs="0" maxOccurs="unbounded"/>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="companyType">
		<xsi:all minOccurs="1">
			<xsi:element name="companyName" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="56"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="referenceNumber" minOccurs="1" maxOccurs="1">
				<xsi:simpleType>
					<xsi:restriction base="xsi:string">
						<xsi:length value="20"/>
					</xsi:restriction>
				</xsi:simpleType>
			</xsi:element>
			<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="1" maxOccurs="1"/>
			<xsi:element name="telephoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1"/>
		</xsi:all>
	</xsi:complexType>
	<xsi:complexType name="assetsType">
		<xsi:choice>
			<xsi:element name="monetaryAssets" type="monetaryAssetsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="propertyAssets" type="propertyAssetsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="shareAssets" type="shareAssetsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="businessAssets" type="businessAssetsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="partnershipAssets" type="partnershipAssetsType" minOccurs="0" maxOccurs="1"/>
			<xsi:element name="otherAssets" type="otherAssetsType" minOccurs="0" maxOccurs="1"/>
		</xsi:choice>
	</xsi:complexType>
	<xsi:complexType name="monetaryAssetsType">
		<xsi:sequence>
			<xsi:element name="monetaryAsset" minOccurs="0" maxOccurs="unbounded">
				<xsi:complexType>
					<xsi:all>
						<xsi:element name="value" minOccurs="1" maxOccurs="1">
							<xsi:simpleType>
								<xsi:restriction base="xsi:decimal">
									<xsi:minInclusive value="0.00"/>
									<xsi:maxExclusive value="1000000000000.00"/>
									<xsi:fractionDigits value="2"/>
								</xsi:restriction>
							</xsi:simpleType>
						</xsi:element>
					</xsi:all>
				</xsi:complexType>
			</xsi:element>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="propertyAssetsType">
		<xsi:sequence>
			<xsi:element name="propertyAsset" minOccurs="0" maxOccurs="unbounded">
				<xsi:complexType>
					<xsi:all>
						<xsi:element name="buildingLandName" minOccurs="1" maxOccurs="1">
							<xsi:simpleType>
								<xsi:restriction base="xsi:string">
									<xsi:length value="10"/>
								</xsi:restriction>
							</xsi:simpleType>
						</xsi:element>
						<xsi:element name="correspondenceAddress" type="correspondenceAddressType" minOccurs="1" maxOccurs="1"/>
						<xsi:element name="propertyLandValue" minOccurs="1" maxOccurs="1">
							<xsi:simpleType>
								<xsi:restriction base="xsi:decimal">
									<xsi:minInclusive value="0.00"/>
									<xsi:maxExclusive value="1000000000000.00"/>
									<xsi:fractionDigits value="2"/>
								</xsi:restriction>
							</xsi:simpleType>
						</xsi:element>
						<xsi:element name="propertyLandEvalDate" type="xsi:date" minOccurs="0" maxOccurs="1"/>
					</xsi:all>
				</xsi:complexType>
			</xsi:element>
		</xsi:sequence>
	</xsi:complexType>
	<xsi:complexType name="shareAssetsType">
		<xsi:sequence>
			<xsi:element name="shareAsset" minOccurs="0" maxOccurs="unbounded">
				<xsi:complexType>
					<xsi:all>
						<xsi:element name="numberShares" type="xsi:integer" minOccurs="1" maxOccurs="1"/>
						<xsi:element name="shareType" minOccurs="1" maxOccurs="1">
							<xsi:simpleType>
								<xsi:annotation>
									<xsi:documentation>Possible share types
										Code   Title
										----   -----
										0001    Ordinary shares
										0002 	Non-voting shares
										0003 	Redeemable shares
										0004	Preference shares
										0005 	Deferred ordinary shares
										0006 	Management shares
										0007 	Other classes of shares
										0008 	Voting shares
										0009 	Dividend shares
										0010 	Capital share
										0011 	Other
									</xsi:documentation>
								</xsi:annotation>
								<xsi:restriction base="xsi:string">
									<xsi:enumeration value="0001"/>
