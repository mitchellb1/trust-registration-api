    "deceasedType": {
      "description": "Deceased Individual Type",
      "additionalProperties": false,
      "properties": {
        "correspondenceAddress": {
          "$ref": "#/definitions/correspondenceAddressType"
        },
        "dateOfDeath": {
          "type": "string"
        },
        "individual": {
          "$ref": "#/definitions/individualType"
        }
      },
      "type": "object"
    },
    "declarationType": {
      "description": "Declaration Type",
      "additionalProperties": false,
      "properties": {
        "correspondenceAddress": {
          "$ref": "#/definitions/correspondenceAddressType"
        },
        "confirmation": {
          "type": "boolean"
        },
        "date": {
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
          "type": "string"
        },
        "familyName": {
          "maxLength": 47,
          "type": "string"
        },
        "givenName": {
          "maxLength": 25,
          "type": "string"
        },
        "otherName": {
          "maxLength": 25,
          "type": "string"
        }
      },
      "required" : [
        "givenName", "familyName", "correspondenceAddress", "date", "confirmation"
      ],
      "type": "object"
    },
    "directorBeneficiariesType": {
      "description": "Director Beneficiaries Type",
      "additionalProperties": false,
      "properties": {
        "directorBeneficiaries": {
          "type": "array",
          "items": {
            "additionalProperties": false,
            "properties": {
              "income": {
                "$ref": "#/definitions/incomeType"
              },
              "individual": {
                "$ref": "#/definitions/individualType"
              },
              "isVulnerable": {
                "type": "boolean"
              }
            },
            "type": "object"
          }
        }
      },
      "type": "object"
    },
    "employeeBeneficiariesType": {
      "description": "Employee Beneficiaries Type",
      "additionalProperties": false,
      "properties": {
        "employeeBeneficiaries": {
          "type": "array",
          "items": {
            "additionalProperties": false,
            "properties": {
              "income": {
                "$ref": "#/definitions/incomeType"
              },
              "individual": {
                "$ref": "#/definitions/individualType"
              },
              "isVulnerable": {
                "type": "boolean"
              }
            },
            "type": "object"
          }
        }
      },
      "type": "object"
    },
    "employmentTrustType": {
      "description": "Employee Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "businessAssets": {
              "$ref": "#/definitions/businessAssetsType"
            },
            "monetaryAssets": {
              "$ref": "#/definitions/monetaryAssetsType"
            },
            "otherAssets": {
              "$ref": "#/definitions/otherAssetsType"
            },
            "propertyAssets": {
              "$ref": "#/definitions/propertyAssetsType"
            },
            "shareAssets": {
              "$ref": "#/definitions/shareAssetsType"
            }
          },
          "type": "object"
        },
        "beneficiaries": {
          "additionalProperties": false,
          "properties": {
            "directorBeneficiaries": {
              "$ref": "#/definitions/directorBeneficiariesType"
            },
            "employeeBeneficiaries": {
              "$ref": "#/definitions/employeeBeneficiariesType"
            },
            "individualBeneficiaries": {
              "$ref": "#/definitions/individualBeneficiariesType"
            },
            "otherBeneficiaries": {
              "$ref": "#/definitions/otherBeneficiariesType"
            }
          },
          "type": "object"
        },
        "employerFinancedRetirementBenefitSchemeStartDate": {
          "type": "string"
        },
        "isEmployerFinancedRetirementBenefitScheme": {
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "estateType": {
      "description": "Estate Type",
      "additionalProperties": false,
      "properties": {
        "estateName": {
          "type": "string"
        },
        "adminPeriodFinishedDate": {
          "type": "boolean"
        },
        "deceased": {
          "$ref": "#/definitions/deceasedType"
        },
        "estateCriteriaMet": {
          "type": "boolean"
        },
        "incomeTaxDueMoreThan10000": {
          "type": "boolean"
        },
        "isCreatedByWill": {
          "type": "boolean"
        },
        "personalRepresentative": {
          "$ref": "#/definitions/personalRepresentativeType"
        },
        "saleOfEstateAssetsMoreThan250000": {
          "type": "boolean"
        },
        "saleOfEstateAssetsMoreThan500000": {
          "type": "boolean"
        },
        "worthMoreThanTwoAndHalfMillionAtTimeOfDeath": {
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "flatManagementSinkingFundTrustType": {
      "description": "Flat Management or Sinking Fund Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "monetaryAssets": {
              "$ref": "#/definitions/monetaryAssetsType"
            }
          },
          "required": ["monetaryAssets"],
          "type": "object"
        },
        "beneficiaries": {
          "additionalProperties": false,
          "properties": {
            "otherBeneficiaries": {
              "$ref": "#/definitions/otherBeneficiariesType"
            }
          },
          "required": ["otherBeneficiaries"],
          "type": "object"
        }
      },
      "required": ["assets", "beneficiaries"],
      "type": "object"
    },
    "heritageMaintenanceFundTrustType": {
      "description": "heritage Maintenance Fund Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "monetaryAssets": {
              "$ref": "#/definitions/monetaryAssetsType"
            },
            "otherAssets": {
              "$ref": "#/definitions/otherAssetsType"
            },
            "propertyAssets": {
              "$ref": "#/definitions/propertyAssetsType"
            },
            "shareAssets": {
              "$ref": "#/definitions/shareAssetsType"
            }
          },
          "anyOf" : [
            {"required": ["monetaryAssets"]},
            {"required": ["shareAssets"]},
            {"required": ["propertyAssets"]}
          ],
          "type": "object"
        },
        "beneficiaries": {
          "additionalProperties": false,
          "properties": {
            "buildingLandBeneficiary": {
              "$ref": "#/definitions/buildingLandBeneficiariesType"
            },
            "otherBeneficiaries": {
              "$ref": "#/definitions/otherBeneficiariesType"
            }
          },
          "anyOf" : [
            {"required": ["buildingLandBeneficiary"]},
            {"required": ["otherBeneficiaries"]}
          ],
          "type": "object"
        },
        "isMultiPurposeIncome": {
          "type": "boolean"
        }
      },
      "type": "object",
      "required": ["assets", "beneficiaries", "isMultiPurposeIncome"]
    },
    "incomeType": {
      "description": "Income Type",
      "additionalProperties": false,
      "properties": {
        "isIncomeAtTrusteeDiscretion": {
          "type": "boolean"
        },
        "shareOfIncome": {
          "type": "number"
        }
      },
      "type": "object"
    },
    "individualBeneficiariesType": {
      "description": "Individual Beneficiaries Type",
      "additionalProperties": false,
      "properties": {
        "individualBeneficiaries": {
          "type": "array",
          "items": {
            "additionalProperties": false,
            "properties": {
              "income": {
                "$ref": "#/definitions/incomeType"
              },
              "individual": {
                "$ref": "#/definitions/individualType"
              },
              "isVulnerable": {
                "type": "boolean"
              }
            },
            "type": "object"
          }
        }
      },
      "type": "object"
    },
    "individualType": {
      "description": "Individual Type",
      "additionalProperties": false,
      "properties": {
        "correspondenceAddress": {
          "$ref": "#/definitions/correspondenceAddressType"
        },
        "dateOfBirth": {
          "type": "string",
          "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$"
        },
        "familyName": {
          "maxLength": 47,
          "type": "string"
        },
        "givenName": {
          "maxLength": 25,
          "type": "string"
        },
        "isUkNationalOrNonUkWithANino": {
          "type": "boolean"
        },
        "nino": {
          "pattern" : "^(?!BG)(?!GB)(?!NK)(?!KN)(?!TN)(?!NT)(?!ZZ)(?:[A-CEGHJ-PR-TW-Z][A-CEGHJ-NPR-TW-Z])(?:\\d){6}([A-D]|\\s)?$",
          "type": "string"
        },
        "otherName": {
          "maxLength" : 25,
          "type": "string"
        },
        "passportOrIdCard": {
          "additionalProperties": false,
          "properties": {
            "countryOfIssue": {
              "$ref": "#/definitions/countryCodeType"
            },
            "expiryDate": {
              "pattern": "^([\\d]{4,4}\\-[\\d]{2,2}\\-[\\d]{2,2})$",
              "type": "string"
            },
            "referenceNumber": {
              "maxLength" : 30,
              "type": "string"
            }
          },
          "type": "object"
        }
      },
      "required" : [
        "givenName", "familyName"
      ],
      "type": "object"
    },
    "interVivoTrustType": {
      "description": "Inter Vivo Trust Type",
      "additionalProperties": false,
      "properties": {
        "assets": {
          "additionalProperties": false,
          "properties": {
            "businessAssets": {
              "$ref": "#/definitions/businessAssetsType"
            },
            "monetaryAssets": {
              "$ref": "#/definitions/monetaryAssetsType"
            },
            "otherAssets": {
              "$ref": "#/definitions/otherAssetsType"
            },
            "partnershipAssets": {
              "$ref": "#/definitions/partnershipAssetsType"
            },
            "propertyAssets": {
              "$ref": "#/definitions/propertyAssetsType"
            },
            "shareAssets": {
              "$ref": "#/definitions/shareAssetsType"
            }
          },
          "type": "object"
        },
        "beneficiaries": {
          "additionalProperties": false,
          "properties": {
            "charityBeneficiaries": {
              "$ref": "#/definitions/charityBeneficiariesType"
            },
            "individualBeneficiaries": {
              "$ref": "#/definitions/individualBeneficiariesType"
            },
            "otherBeneficiaries": {
              "$ref": "#/definitions/otherBeneficiariesType"
            }
          },
          "type": "object"
        },
        "isHoldOverClaimed": {
          "type": "boolean"
        }
      },
      "type": "object"
    },
    "leadTrusteeType": {
      "description": "Lead Trustee Type",
      "additionalProperties": false,
      "properties": {
        "company": {
          "$ref": "#/definitions/companyType"
        },
        "individual": {
          "$ref": "#/definitions/individualType"
        },
        "telephoneNumber": {
          "$ref": "#/definitions/telephoneNumberType"
        },
        "email" : {
          "$ref": "#/definitions/emailType"
        }
      },
      "type": "object",
      "oneOf": [
        {"required": ["individual", "telephoneNumber", "email"]},
        {"required": ["company", "telephoneNumber", "email"]}
      ]
    },
    "legalityType": {
      "description": "Legality Type",
      "additionalProperties": false,
      "properties": {
        "administrationCountryCode": {
          "$ref": "#/definitions/countryCodeType"
        },
        "governingCountryCode": {
          "$ref": "#/definitions/countryCodeType"
        },
        "isEstablishedUnderScottishLaw": {
          "type": "boolean"
        },
        "previousOffshoreCountryCode": {
          "$ref": "#/definitions/countryCodeType"
        }
      },
      "required" : [
        "governingCountryCode",
        "isEstablishedUnderScottishLaw"
      ],
      "type": "object"
    },
    "monetaryAssetsType": {
      "description": "Monetary Assets Type",
      "type": "array",
      "items": {
        "value": {
          "type": "number"
        }
      },
      "minItems": 1
    },
    "naturalPeopleType": {
      "description": "Natural People Individual Type",
      "additionalProperties": false,
      "properties": {
        "individuals": {
          "type": "array",
          "minItems": 0,
          "maxItems": 2,
          "items": {
            "$ref": "#/definitions/individualType"
          }
        }
      }
    },
    "otherAssetsType": {
      "description": "Other Assets Type",
      "additionalProperties": false,
      "properties": {
        "otherAssets": {
          "type": "array",
          "items": {
            "additionalProperties": false,
            "properties": {
              "OtherAssetDescription": {
                "type": "string"
              },
              "lastValuationDate": {
                "type": "string"
              },
              "value": {
                "type": "number"
              }
            },
            "type": "object"
          }
        }
      },
      "type": "object"
    },
    "otherBeneficiariesType": {
      "description": "Other Beneficiaries Type",
      "additionalProperties": false,
      "type": "array",
      "minItems": 1,
      "items": {
        "additionalProperties": false,
        "properties": {
          "beneficiaryDescription": {
            "maxLength": 56,
            "type": "string"
          },
          "correspondenceAddress": {
            "$ref": "#/definitions/correspondenceAddressType"
          }
        },
        "type": "object",
        "required": ["beneficiaryDescription", "correspondenceAddress"]
      }
    },
    "partnershipAssetsType": {
      "description": "Partnership Assets Type",
      "additionalProperties": false,
      "properties": {
